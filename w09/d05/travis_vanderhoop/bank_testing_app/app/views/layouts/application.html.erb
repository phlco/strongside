<!DOCTYPE html>
<html>
<head>
  <title>BankTesting</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
    <li>
      <%= link_to('Logout', destroy_user_session_path, :method => :delete) %>
    </li>
  <% end %>
 <p class="notice"><%= notice %></p>
 <p class="alert"><%= alert %></p>
  <%= yield %>

</body>
</html>
