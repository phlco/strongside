<h1>cats</h1>

<ul>

  <% @cats.each do |cat| %>
    <%= image_tag( cat.img_url ) %>
    <li>name:<%= cat.name %></li>
    <li>age:<%= cat.age %></li>
    <li>score:<%= cat.score %></li>
    <li><%= link_to 'Show', cat %></li>
    <li><%= link_to 'Edit', edit_cat_path(cat) %></li>
<% end %>


</ul>
<br />

<%= link_to 'New Cat', new_cat_path %>
